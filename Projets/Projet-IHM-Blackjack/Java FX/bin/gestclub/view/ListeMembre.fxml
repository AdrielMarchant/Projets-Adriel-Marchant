<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171">
   <top>
      <Label text="Gestion des membres" BorderPane.alignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </top>
   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: lightblue;" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <ButtonBar prefHeight="72.0" prefWidth="510.0" styleClass="Bbar" stylesheets="@../resource/style.css" BorderPane.alignment="CENTER">
        <buttons>
          <Button mnemonicParsing="false" text="Nouveau membre" />
            <Button mnemonicParsing="false" text="Editer" />
            <Button mnemonicParsing="false" prefHeight="25.0" text="Supprimer">
               <opaqueInsets>
                  <Insets right="10.0" />
               </opaqueInsets>
            </Button>
        </buttons>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </ButtonBar>
   </bottom>
</BorderPane>
